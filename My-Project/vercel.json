{
  "rewrites": [
    {
      "source": "/My-Project/assets/:file*",
      "destination": "/assets/:file*"
    },
    {
      "source": "/My-Project/",
      "destination": "/"
    },
    {
      "source": "/My-Project/:path*",
      "destination": "/:path*"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ]
}
